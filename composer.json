{
    "name": "grimaceofdespair/import-kyero-feed",
    "description": "Wordpress plugin to import Easy Real Estate properties and images from a Kyero feed.",
    "keywords": [
        "importer", "wordpress", "kyero"
    ],
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "homepage": "https://wordpress.org/plugins/import-kyero-feed/",
    "support": {
        "issues": "https://wordpress.org/support/plugin/import-kyero-feed/",
        "source": "https://github.com/GrimaceOfDespair/kyero-importer"
    },
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "^2.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
        "phpcompatibility/phpcompatibility-wp": "^2.1",
        "yoast/phpunit-polyfills": "^1.0.1",
        "phpunit/phpunit": "^5.7.21 || ^6.5 || ^7.5"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "phpunit": "phpunit",
        "kyero": "phpunit --debug --filter test_kyero_import"
    }    
}
